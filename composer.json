{
    "name": "vctls/adldap2-bundle",
    "description": "Symfony Bundle for adldap2 library.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Toulouse",
            "email": "victor.toulouse@insa-toulouse.fr"
        }

    ],
    "autoload": {
        "psr-4": { "Vctls\\Adldap2Bundle\\": "" }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/vctls/adldap2-bundle.git"
        }
    ],
    "require": {
        "php"                           : ">=5.4",
        "adldap2/adldap2"               : "~6.1",
        "symfony/config"                : "~2.3|~3.0",
        "symfony/dependency-injection"  : "~2.3|~3.0",
        "symfony/http-kernel"           : "~2.3|~3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
